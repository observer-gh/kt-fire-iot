{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "version": {
      "type": "integer"
    },
    "alert_id": {
      "type": "string"
    },
    "channel": {
      "type": "string"
    },
    "recipient": {
      "type": ["null", "string"]
    },
    "sent_at": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": ["version", "alert_id", "channel", "sent_at"]
}
